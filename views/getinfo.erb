<div id="dailyFeed">
	<% @info.each do |post|
		current_row = post.split(",")
		date = current_row[0].to_i
		date = Time.at(date).strftime('%x %I:%M:%S%p') %>
		<div class ="post thinbluebox">
			<div class = "postProfile">
				<div class="postname"><%=current_row[2]%></div>
				<div class="postdate"><%=date%></div>
			</div>
			<div class="postsubmission"><span class="post_info">Time since previous post: <%=current_row[1]%><br>
				Stress Level: <%=current_row[3]%></span><br>
				<%=current_row[4]%>
			</div>
		</div>
	<%end%>
</div>
